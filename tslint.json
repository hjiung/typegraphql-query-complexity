{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "max-line-length": {
            "severity": "warning",
            "options": [
                120
            ]
        },
        "object-literal-sort-keys": {
            "severity": "warning",
            "options": [
                "ignore-case"
            ]
        },
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}